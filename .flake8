[flake8]
 ignore = E501, E265, F811, PT001, DJ05, D100, D105, D104, W504, W292
 max-line-length = 79
 paths = 
    ./yacut
 max-complexity = 10
 exclude = 
    tests
    migrations
 per-file-ignores =
    __init__.py: I004, F401, E402
    /yacut/serializers.py: R506
    /yacut/forms.py: I001, I005
    /yacut/utils/error_handlers.py: N818
